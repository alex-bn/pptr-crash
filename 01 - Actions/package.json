{
  "name": "pptr",
  "version": "1.0.0",
  "description": "Automated testing framework with Puppeteer",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./Snippets/example.test.js",
    "device": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./Snippets/device.test.js",
    "e2e": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./Snippets/e2e.test.js",
    "test:e2e": "node ./node_modules/mocha/bin/mocha --timeout=30000 ./e2e/*.test.js"
  },
  "keywords": [
    "Puppeteer",
    "Mocha",
    "Chai",
    "Automation",
    "Jest"
  ],
  "author": "Alex Bajan",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.4",
    "mocha": "^9.1.2",
    "puppeteer": "^10.4.0"
  }
}
